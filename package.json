{
  "name": "castaways-plugin",
  "private": true,
  "version": "1.0.0",
  "description": "Build scripts for Castaways plugin assets",
  "scripts": {
    "build:css": "postcss assets/css/castawaystravel.css -o assets/css/castawaystravel.min.css --map",
    "build:js": "terser assets/js/castawaystravel.js -o assets/js/castawaystravel.min.js --source-map",
    "build": "npm run build:css && npm run build:js"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "terser": "^5.24.0"
  },
  "browserslist": [
    "defaults"
  ]
}
